<!DOCTYPE html>
<html>

<head>
  <title>LayoutsAssignment</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
      <%= csrf_meta_tags %>
</head>

<body>

<div class="container">
<div class="row">
        <div class="col-sm-4">
            <%= yield :all_posts %>
        </div>
        <div class="col-sm-4">
            <%= yield :new_post %>
        </div>
        <div class="col-sm-4">
            <%= yield :all_users %>
        </div>
    </div>
</div>
    
</body>

</html>