<h1>Editting <%= @dojo.branch %></h1>
<% for message in flash[:message] %>
    <p> <%= message %> </p>
<% end %>
<div>
    <form action="/dojos/<%= @dojo.id %>" method="post">
        

        <div class="labels">
            <p><label>Branch: </label></p>
            <p><label>Street: </label></p>
            <p><label>City: </label></p>
            <p><label>State: </label></p>

        </div>

        <div class="inputs">
            <input type="hidden" name="_method" value="patch">
            <input type="hidden" name="authenticity_token" value="<%= form_authenticity_token %>">
            <p><input type="text" name="dojo[branch]" value = "<%= @dojo.branch %>"</p>
            <p><input type="text" name="dojo[street]" value = "<%= @dojo.street %>"></p>
            <p><input type="text" name="dojo[city]" value = "<%= @dojo.city %>"></p>
            <p><input type="text" name="dojo[state]" value = "<%= @dojo.state %>"></p>
            <p><input type="submit"></p>

        </div>
    </form>
</div>

<p> <a href="/dojos">Home</a> &nbsp; <a href="/dojos/<%= @dojo.id %>"> Show</a> </p>